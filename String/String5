class Solution
{
    //Function to check if two strings are rotations of each other or not.
    public static boolean areRotations(String s1, String s2 )
    {
       String s3=s1+s1;
        if(s1.length()!=s2.length())
        {
            return false;
        }
        else if(s3.contains(s2))
        {
            return true;
        }
        else return false;
    }
    
}
